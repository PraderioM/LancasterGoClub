    "place": "ROOM NAME", // This is the actual room within Lancaster University in which you will be meeting.
    "extraInformation": "", // Add any additional information here. For example if this is a temporary location write it here or if the meeting is online write it here.
    "linkToPlace": "LINK TO MAP"  // A link to Maze Map or other pointing to the meeting location. If it is online make it point to the online meeting place.
  }


